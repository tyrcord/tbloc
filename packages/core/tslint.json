{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-over-type-literal": false,
    "no-empty": [
      true,
      "allow-empty-functions"
    ],
    "cyclomatic-complexity": [
      true,
      5
    ],
    "deprecation": true,
    "max-file-line-count": [
      true,
      300
    ]
  }
}
